{
  "manifest_version": 3,
  "name": "Storage Exporter",
  "version": "1.0",
  "description": "Export localStorage, sessionStorage and Cookies",
  "permissions": ["activeTab", "scripting", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon16.png",
      "128": "icons/icon16.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  }
}